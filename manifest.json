{
   "name": "Jimmy Launcher",
   "version": "1.0",
   "description": "Launch your favorite sites",
   "permissions": [
      "declarativeContent",
      "storage",
      "tabs",
      "activeTab",
      "<all_urls>"
   ],
   "background": {
      "scripts": [
         "background.js"
      ],
      "persistent": false
   },
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
   },
   
   "icons": {
      "16": "images/icon.png",
      "128": "images/icon_128.png"
   },
   "manifest_version": 2
}